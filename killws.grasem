killws {
  output = asonToken+
  asonToken = string | delimiter | integer | namedMonth | hex | unknown | ident | ws

  string = "S" text eol
  delimiter = "D" subDelimiter eol
  integer = "I" text eol
  namedMonth = "M" text eol
  hex = "H" text eol
  unknown = "U" eol
  ident = "N" text eol
  ws = "W" text eol

  subDelimiter = sub text
	      
  sub = "a" .. "o"

  text = ~eol whiteSpace* any
  eol = "\n"+
  whiteSpace = " " | "\t"
}
